{
    "name": "phlexible/element-finder-bundle",
    "description": "phlexible element finder bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["find"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\ElementFinderBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "doctrine/collections": "^1.3",
        "doctrine/dbal": "^2.0",
        "fluentdom/fluentdom": "^5.1",
        "phlexible/element-bundle": "~1.3",
        "phlexible/gui-bundle": "~1.3",
        "phlexible/tree-bundle": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "mikey179/vfsStream": "^1.4",
        "symfony/dependency-injection": "^2.8",
        "matthiasnoback/symfony-config-test": "^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
